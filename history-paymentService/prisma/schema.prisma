generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model HistoryPayment {
  id            Int      @id @default(autoincrement())
  transactionId Int
  userId        String
  amount        Int
  method        String?
  status        String
  createdAt     DateTime @default(now())
}
